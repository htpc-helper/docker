export LC_ALL=C
export DEBIAN_FRONTEND=noninteractive
export TERM=xterm
export LANG=en_AU.UTF-8
export LANGUAGE=en_AU
minimal_apt_get_install='apt-get install -qq -y --no-install-recommends'

# Default services
# Set 1 to the service you want to disable
export DISABLE_SYSLOG=${DISABLE_SYSLOG:1}
export DISABLE_SSH=${DISABLE_SSH:1}
export DISABLE_CRON=${DISABLE_CRON:1}
